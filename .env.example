# OpenAI API Key for the chatbot (optional, not required for local FAQ bot)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Backend email (SMTP) configuration for contact form
# These are used by the Node server (not exposed to the browser)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password
TO_EMAIL=you@example.com
PORT=7777
